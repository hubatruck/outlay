<?php

namespace {{ namespace }};

use ArielMejiaDev\LarapexCharts\LarapexChart;
use ArielMejiaDev\LarapexCharts\RadialChart;

class {{ class }} extends BaseChart
{
    public function build(): RadialChart
    {
        return $this->chart
            ->radialChart()
            ->setTitle('Passing effectiveness.')
            ->setSubtitle('Barcelona city vs Madrid sports.')
            ->addData([75, 60])
